# Voyage AI Configuration
# Required: Get your API key from https://www.voyageai.com/
VOYAGE_API_KEY=your_voyage_api_key_here
VOYAGE_MODEL=voyage-3.5
VOYAGE_BATCH_SIZE=128

# Processing Configuration
PROCESSING_CHUNK_SIZE=1000
VECTOR_DIMENSION=1024

# Qdrant Configuration
# Required: Your Qdrant server URL
QDRANT_URL=http://localhost:6333
# Optional: Qdrant API key (if using cloud or authenticated instance)
QDRANT_API_KEY=
QDRANT_COLLECTION=candidates_store
QDRANT_HOST=
QDRANT_PORT=80

# Anthropic Claude Configuration
# Required: Get your API key from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-5-haiku-20241022
ANTHROPIC_BATCH_SIZE=128
ANTHROPIC_MAX_WORKERS=50

# App Configuration
APP_NAME=Vector Ingestion Service
DEBUG=false

